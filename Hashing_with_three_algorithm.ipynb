      },
      "source": [
        "Hashing = input(\"Enter the word to be Hashed : \")\n",
        "\n",
        "enc = hashlib.sha3_224(Hashing.encode())\n",
        "\n",
        "enc = hashlib.sha1(Hashing.encode())\n",
        "\n",
        "enc = hashlib.blake2b(Hashing.encode())\n",
        "\n",
        "print(\"HASHED FORM :\",enc.hexdigest())"
      ],
